{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vate",
  "version": "1.1.0",
  "identifier": "com.vamamurin.vate",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "frontendDist": "../ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Vate",
        "width": 860,
        "height": 700,
        "decorations": false,
        "transparent": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
